#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ma7dev/Vault/Cursor/slides/.build/node_modules/.pnpm/@slidev+cli@51.8.2_@babel+parser@7.28.5_@nuxt+kit@3.20.2_@types+markdown-it@14.1.2_@typ_34bce97725a710bed75b80cc7185e248/node_modules/@slidev/cli/bin/node_modules:/Users/ma7dev/Vault/Cursor/slides/.build/node_modules/.pnpm/@slidev+cli@51.8.2_@babel+parser@7.28.5_@nuxt+kit@3.20.2_@types+markdown-it@14.1.2_@typ_34bce97725a710bed75b80cc7185e248/node_modules/@slidev/cli/node_modules:/Users/ma7dev/Vault/Cursor/slides/.build/node_modules/.pnpm/@slidev+cli@51.8.2_@babel+parser@7.28.5_@nuxt+kit@3.20.2_@types+markdown-it@14.1.2_@typ_34bce97725a710bed75b80cc7185e248/node_modules/@slidev/node_modules:/Users/ma7dev/Vault/Cursor/slides/.build/node_modules/.pnpm/@slidev+cli@51.8.2_@babel+parser@7.28.5_@nuxt+kit@3.20.2_@types+markdown-it@14.1.2_@typ_34bce97725a710bed75b80cc7185e248/node_modules:/Users/ma7dev/Vault/Cursor/slides/.build/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ma7dev/Vault/Cursor/slides/.build/node_modules/.pnpm/@slidev+cli@51.8.2_@babel+parser@7.28.5_@nuxt+kit@3.20.2_@types+markdown-it@14.1.2_@typ_34bce97725a710bed75b80cc7185e248/node_modules/@slidev/cli/bin/node_modules:/Users/ma7dev/Vault/Cursor/slides/.build/node_modules/.pnpm/@slidev+cli@51.8.2_@babel+parser@7.28.5_@nuxt+kit@3.20.2_@types+markdown-it@14.1.2_@typ_34bce97725a710bed75b80cc7185e248/node_modules/@slidev/cli/node_modules:/Users/ma7dev/Vault/Cursor/slides/.build/node_modules/.pnpm/@slidev+cli@51.8.2_@babel+parser@7.28.5_@nuxt+kit@3.20.2_@types+markdown-it@14.1.2_@typ_34bce97725a710bed75b80cc7185e248/node_modules/@slidev/node_modules:/Users/ma7dev/Vault/Cursor/slides/.build/node_modules/.pnpm/@slidev+cli@51.8.2_@babel+parser@7.28.5_@nuxt+kit@3.20.2_@types+markdown-it@14.1.2_@typ_34bce97725a710bed75b80cc7185e248/node_modules:/Users/ma7dev/Vault/Cursor/slides/.build/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@slidev/cli/bin/slidev.mjs" "$@"
else
  exec node  "$basedir/../@slidev/cli/bin/slidev.mjs" "$@"
fi
